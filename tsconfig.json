{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@services/*": ["./src/services/*"],
      "@constants/*": ["./src/constants/*"],
      "@types": ["./src/types/index.ts"]
    }
  },
  "exclude": [
    "./dist",
    "./.quasar",
    "./node_modules",
    "./src-capacitor",
    "./src-cordova",
    "./quasar.config.*.temporary.compiled*"
  ]
}
