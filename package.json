    "vue": "^3.4.37",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^4.0.0",
