{
  "name": "ff24-saving",
  "version": "1.0.0",
  "description": "Calculate tax from saving account of ff24",
  "productName": "ff24 calculator",
  "author": "Dmytro Mysak <dmytro.mysak@cyberproof.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "start": "quasar dev",
    "prebuild": "tsc --noEmit --skipLibCheck",
    "build": "quasar build",
    "cz": "cz",
    "update": "ncu -u",
    "semantic-release": "semantic-release",
    "predeploy": "npm run build",
    "deploy": "vercel",
    "prepare": "husky"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "@vueuse/core": "^10.8.0",
    "quasar": "^2.14.5",
    "vue": "^3.4.19",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@netly/eslint-config-base": "^1.1.2",
    "@netly/prettier-config": "^1.3.0",
    "@netly/semantic-release-config": "^1.0.3",
    "@quasar/app-vite": "^1.7.3",
    "@types/node": "^20.11.19",
    "autoprefixer": "^10.4.17",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-check-updates": "^16.14.15",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18 || >=20",
    "npm": ">= 9.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,js,vue,html,css}": "prettier --write",
    "src/**/*.{ts,js,vue}": "eslint --fix"
  },
  "prettier": "@netly/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
