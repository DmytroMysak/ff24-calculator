{
  "name": "ff24-saving",
  "version": "1.0.0",
  "description": "Calculate tax from saving account of ff24",
  "productName": "FF24++",
  "author": "Dmytro Mysak <dmytro.mysak@cyberproof.com>",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "start": "quasar dev",
    "prebuild": "tsc --noEmit --skipLibCheck",
    "build": "quasar build",
    "test": "vitest",
    "cz": "cz",
    "update": "ncu -u",
    "semantic-release": "semantic-release",
    "predeploy": "npm run build",
    "deploy": "vercel",
    "prepare": "husky"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^10.9.0",
    "nodemailer": "^6.9.13",
    "quasar": "^2.15.1",
    "vue": "^3.4.21",
    "vue-i18n": "^9.10.2",
    "vue-router": "^4.3.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.1/xlsx-0.20.1.tgz"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^3.0.1",
    "@netly/eslint-config-base": "^1.1.2",
    "@netly/prettier-config": "^1.3.0",
    "@netly/semantic-release-config": "^1.0.3",
    "@quasar/app-vite": "^2.0.0-beta.5",
    "@types/node": "^20.11.30",
    "autoprefixer": "^10.4.19",
    "eslint-plugin-vue": "^9.24.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-check-updates": "^16.14.18",
    "typescript": "~5.3.0",
    "vite-plugin-checker": "^0.6.4",
    "vue-tsc": "^2.0.7"
  },
  "engines": {
    "node": ">=18 || >=20",
    "npm": ">= 9.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,js,vue,html,css}": "prettier --write",
    "src/**/*.{ts,js,vue}": "eslint --fix"
  },
  "prettier": "@netly/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
